{
  "name": "royal-flush",
  "version": "1.0.0",
  "description": "poker odds api running using nodejs and pokerstove",
  "main": "index.js",
  "author": "jaimin darji",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest",
    "watch-ts": "tsc -w",
    "watch-sls": "sls offline",
    "start": "concurrently -k -p \"[{name}]\" -n \"TypeScript,SLS\" -c \"cyan.bold,yellow.bold\" \"npm run watch-ts\" \"npm run watch-sls\""
  },
  "dependencies": {
    "es-iter": "^1.1.1",
    "lodash": "^4.17.10",
    "random-js": "^1.0.8",
    "shelljs": "^0.8.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.11",
    "@types/core-js": "^2.5.0",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.7.1",
    "@types/random-js": "^1.0.30",
    "@types/shelljs": "^0.8.0",
    "concurrently": "^4.0.1",
    "jest": "^23.5.0",
    "serverless-offline": "^3.25.11",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.10.0",
    "typescript": "^3.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/tests/"
    ]
  }
}